<div class="modal fade" id="fullreview" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title" id="staticBackdropLabel">#Review</h3>
        <button type="button" data-bs-dismiss="modal" aria-label="Close" style="color: gold;background-color:#221f1f;border: 0;"><i class="fa-solid fa-xmark fa-2xl"></i></button>
      </div>
      <div class="modal-body">
        <div class="reviewbox">
          <div class="box-top">   
              <div class="profile">
                  <div class="profile-img">
                      <img src="https://indiaglitz-media.s3.amazonaws.com/telugu/home/rrr-review-250322-1.jpg" />
                  </div>

                    <div class="name-user">
                      <strong>N.T.Rama Rao</strong>
                      <span>#ishuru@gmail.com</span>
                    </div>
              </div>
              <div class="ratingdiv">
        
              </div>
             
          </div>

          <div class="client-comment">
              <p>Theatre is very good at maintainence....</p>
          </div>
      </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

    
        <div class="row" style="margin-top: 2rem;display: flex;justify-content: center;">
           <div class="col-lg-1">

           </div>   
          <div class="col-lg-3 col-md-4 col-sm-6">
              <div class="currentmovies" style="background-color: #221f1f;color:white;border-radius: 5px;width: 15rem;height: 8rem;">
                <h3 style="padding-left: 1rem;padding-top: 0.5rem;">Latest Movies screening</h3>
                <h4 style="padding-left: 1rem;padding-top: 0.5rem;color: gold;">4</h4>
              </div>
          </div> 

          <div class="col-lg-3 col-md-4 col-sm-6">
            <div class="" style="background-color: #221f1f;color: white;border-radius: 5px;width: 15rem;height: 8rem;">
              <h3 style="padding-left: 1rem;padding-top: 0.5rem;">Total Movies Screened</h3>
              <h4 style="padding-left: 1rem;padding-top: 0.5rem;color: gold;">10</h4>
            </div>
          </div>

          <div class="col-lg-3 col-md-4 col-sm-6">
            <div class="currentmovies" style="background-color: #221f1f;color: white;border-radius: 5px;width: 15rem;height: 8rem;">
              <h3 style="padding-left: 1rem;padding-top: 0.5rem;">New Movies</h3>
              <h4 style="padding-left: 1rem;padding-top: 0.5rem;color: gold;">2</h4>
            </div>
          </div>
          
          <div class="col-lg-1">

          </div>   

        </div>


        <div class="container" style="margin-top:5rem;">
          <div class="reviewsandratings">
            <h4>Review and Rating</h4>
            <% if(treviewsarr.length==0){ %>
             <p></p>
            <% } else{%>
              <div class="reviewtable">
                <table>
                  <thead>
                    <tr class="headingrow">
                      <th>#</th>
                      <th>MailId</th>
                      <th>Name</th>
                      <th>Rating</th>
                      <th>Review</th>
                    </tr>
                  </thead>
                  
                  <tbody>
                    <% for( let i=0;i<treviewsarr.length;i++) { %>
                      <tr>
                        <td><%= i+1 %></td>
                        <td><%=treviewsarr[i]["usermailid"]%></td>
                        <td><%=treviewsarr[i]["username"]%></td>
                        <td><%=treviewsarr[i]["rating"]%></td>
                        <td style="display: none;"><%=treviewsarr[i]["reviewdesc"]%></td>
                        <td>
                        <button type="button" class="btn btn-primary seefullreview" data-bs-toggle="modal" data-bs-target="#fullreview">
                            See Full Review
                        </button>
                        </td>
                      </tr>
                    <% } %>   
                  </tbody>
                </table>
            </div>

              <% } %>
            
            
          </div>
        </div>

          <hr style="color: white;width:75%;margin-left: 12rem;margin-top:2rem;">
     
         <div class="footer">
         gtgtbt
         </div>
         
       

  


   
                  

    

       




 <script src="/js/theatredashboard/theatredashboard.js"></script>
