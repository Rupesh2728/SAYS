            <h4 style="width:15rem;font-size:1.5rem">Theatre Verification</h4>
            <%if(theatreverificationarr.length==0){%>
              <p style="color: gold;font-size: 2rem;">No Theatres For Verification</p>     
            <%} else{%>
              <div class="reviewtable">
                <table>
                  <thead>
                    <tr class="headingrow">
                      <th>#</th>
                      <th>Theatre Liscence No.</th>
                      <th>Theatre MailId</th>
                      <th>Theatre Name</th>
                      <th>Location</th>
                      <th>Verification</th>
                    </tr>
                  </thead>
                  
                  <tbody>
                  <%for(let i=0;i<theatreverificationarr.length;i++){%>
                    <tr>
                      <td><%=i+1%></td>
                      <td><%=theatreverificationarr[i]["licensenum"]%></td>
                      <td><%=theatreverificationarr[i]["temail"]%></td> 
                      <td><%=theatreverificationarr[i]["tName"]%></td>
                      <td><%=theatreverificationarr[i]["city"]%></td>
                      <td>
                      <button type="button" class="btn btn-success acceptbtn">
                         Accept
                      </button>

                      <button type="button" class=" btn btn-danger rejectbtn">
                       Reject
                      </button>
                      </td>
                    </tr>
                 <%}%>            
                  </tbody>
                </table>
            </div>
            <%}%>   
