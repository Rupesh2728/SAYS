      <div class="usertable">
        <div class="heading">
            <h4>CLIENTS</h4>
        </div>
      <%if(value.length==0){%>
        <p style="color: gold;font-size: 2rem;text-align: center;">No Clients for the Website...Till Date</p>     
      <%} else{%>
        <div class="container" style="margin-top:2rem;margin-left: 19rem;">
          <div class="clienttable">  
            <table>
              <thead>
                <tr class="headingrow">
                  <th>#</th>
                  <th>Profile</th>
                  <th>CLient ReffNo.</th>
                  <th>CLient MailId</th>
                  <th>ClientName</th>
                </tr>
              </thead>
              
              <tbody>
                <% for(let i=0;i<value.length;i++) {%>
                  <tr>
                    <td><%=i+1%></td>
                    <td><img src="https://cdn.siasat.com/wp-content/uploads/2022/08/jr-ntr-health.jpg" alt="" class="profilepic" style="border: 1.5px solid gold;"></td>
                    <td><%=value[i]["UserReferenceNumber"]%></td>
                    <td><%=value[i]["email"]%></td>
                    <td><%=value[i]["firstName"]%></td>
                    <td>
                      <div class="editremove" style="display: flex;">
                       <button type="button" class="btn btn-primary removebtn" style="margin-left:1rem;">
                         Remove
                      </button>
                      </div> 
                    </td>
                  </tr>
                <%}%>
              </tbody>
            </table>
        </div>      
        </div>
      <%}%>
       
    </div>

    <script src="/js/admin/adminclient.js"></script>
